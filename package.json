{
  "author": "inc2734",
  "license": "GPL-2.0+",
  "devDependencies": {
    "@wordpress/browserslist-config": "^5.8.0",
    "@wordpress/env": "^5.10.0",
    "@wordpress/scripts": "^25.2.0",
    "autoprefixer": "^10.4.13",
    "cpy-cli": "^4.2.0",
    "cssnano": "^5.1.14",
    "node-sass": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "rimraf": "^4.1.1"
  },
  "scripts": {
    "wp-env": "wp-env",
    "start": "npm run build",
    "stop": "wp-env stop",
    "cli": "wp-env run cli",
    "wp": "wp-env run cli wp",
    "composer": "wp-env run composer",
    "pretest": "wp-env start && wp-env run composer 'install --no-interaction'",
    "test:lint:php": "wp-env run composer run-script lint",
    "test:lint": "run-s test:lint:*",
    "test:unit:php": "wp-env run phpunit 'phpunit -c /var/www/html/wp-content/plugins/wp-share-buttons/.phpunit.xml.dist --verbose'",
    "test:unit": "run-s test:unit:*",
    "test": "run-s test:*",
    "cleanup:css": "rimraf src/assets/css",
    "cleanup:js": "rimraf src/assets/js",
    "cleanup:img": "rimraf src/assets/img",
    "css:app:sass": "node-sass src/src/css/wp-share-buttons.scss -o src/assets/css",
    "css:app:postcss": "postcss src/assets/css/wp-share-buttons.css -o src/assets/css/wp-share-buttons.css -c=postcss.config.js",
    "css:app": "npm-run-all -s css:app:*",
    "css": "npm run cleanup:css && npm-run-all -p css:*",
    "js:build": "wp-scripts build src/src/js/wp-share-buttons.js --output-path src/assets/js",
    "js": "npm-run-all -s cleanup:js js:build",
    "img": "npm run cleanup:img && cpy 'src/src/img/*' 'src/assets/img'",
    "build": "npm-run-all -p css js img",
    "makepot": "wp i18n make-pot ./wp-content/plugins/wp-share-buttons/src ./wp-content/plugins/wp-share-buttons/src/languages/generic.pot --domain=inc2734-wp-share-buttons"
  },
  "dependencies": {}
}
